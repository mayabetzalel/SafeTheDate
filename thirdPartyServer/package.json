{
  "name": "server",
  "version": "1.0.0",
  "description": "graphql server",
  "main": "server.ts",
  "scripts": {
    "start": "concurrently --names \"SERVER,CODEGEN\" \"nodemon src/server.ts\" \"npm run generate:watch\"",
    "start:prod": "node /dist/server.js",
    "build": "tsc",
    "prebuild": "npm run generate",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch"
  },
  "author": "SafeTheDate",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fetch": "^1.1.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.8.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.3",
    "node-wit": "^6.6.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@graphql-tools/load-files": "^6.6.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2"
  }
}
